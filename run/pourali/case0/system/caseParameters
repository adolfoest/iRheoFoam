// Case parameters
l		800;

// domain size
x0			#eval{ 0.5*$l };
y0			#eval{ -0.5*$l };
z0			#eval{ 0 };

x1			#eval{-0.5*$l};
y1			0;
z1			#eval{ 0.5*$l };

nx			32;
ny			16;
nz			16;


