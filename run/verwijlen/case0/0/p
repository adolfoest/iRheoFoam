FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    staticRod
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }  
    movingRod
    {
    	type            zeroGradient;
    }     
    front
    {
    	type            zeroGradient;
    } 
    back
    {
    	type            zeroGradient;
    } 
}


